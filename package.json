    "prettier": "^1.14.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
